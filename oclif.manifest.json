      "pluginAlias": "botarena",
      "pluginName": "botarena",
      "pluginType": "core",
      "strict": true,
      "enableJsonFlag": false,
      "isESM": true,
      "relativePath": [
        "dist",
        "cli",
        "commands",
        "upload.js"
      ]
