        },
        "dry-run": {
          "description": "Generate profile without uploading to BotArena",
          "name": "dry-run",
          "allowNo": false,
          "type": "boolean"
        },
        "output": {
          "char": "o",
          "description": "Output file path for generated profile",
          "name": "output",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "verbose": {
          "description": "Enable verbose logging output",
          "name": "verbose",
          "allowNo": false,
          "type": "boolean"
        },
        "path": {
          "char": "p",
          "description": "Path to bot directory",
          "name": "path",
          "default": ".",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        }
      },
      "hasDynamicHelp": false,
      "hiddenAliases": [],
      "id": "generate",
      "pluginAlias": "botarena",
      "pluginName": "botarena",
