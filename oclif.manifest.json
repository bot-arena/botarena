{
  "commands": {
    "generate": {
      "aliases": [],
      "args": {
        "botPath": {
          "description": "Path to bot directory for optional discovery",
          "name": "botPath",
          "required": false
        }
      },
      "description": "Generate a bot profile JSON for BotArena showcase",
      "examples": [
        "<%= config.bin %> <%= command.id %> --name \"Bambus Bot\" --description \"Chill server goblin\" --harness \"ClawdBot\" --llm \"gpt-4o\"",
        "<%= config.bin %> <%= command.id %> --name \"Bambus Bot\" --description \"Chill server goblin\" --harness \"ClawdBot\" --llm \"gpt-4o\" --skills \"search,tools\" --mcps \"filesystem\" --clis \"git\"",
        "<%= config.bin %> <%= command.id %> --interactive"
      ],
      "flags": {
        "interactive": {
          "description": "Run with interactive prompts for bot configuration",
          "name": "interactive",
          "allowNo": false,
          "type": "boolean"
        },
        "output": {
          "char": "o",
          "description": "Output file path for generated profile JSON",
          "name": "output",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "verbose": {
          "description": "Enable verbose logging output",
          "name": "verbose",
          "allowNo": false,
          "type": "boolean"
        },
        "path": {
          "char": "p",
          "description": "Path to bot directory for optional discovery",
          "name": "path",
          "default": ".",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "name": {
          "description": "Bot name (identity)",
          "name": "name",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "avatar": {
          "description": "Avatar URL (optional)",
          "name": "avatar",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "description": {
          "char": "d",
          "description": "Bot description for the profile (yearbook quote style)",
          "name": "description",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "harness": {
          "description": "Bot harness/framework (ex: ClawdBot)",
          "name": "harness",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "model": {
          "description": "Primary LLM model string (alias: --llm)",
          "name": "model",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "llm": {
          "description": "Primary LLM model string (alias: --model)",
          "name": "llm",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "fallbacks": {
          "description": "Comma-separated list of fallback models",
          "name": "fallbacks",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "skills": {
          "description": "Comma-separated list of skills",
          "name": "skills",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "mcps": {
          "description": "Comma-separated list of MCP servers",
          "name": "mcps",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "clis": {
          "description": "Comma-separated list of CLI tools",
          "name": "clis",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        }
      },
      "hasDynamicHelp": false,
      "hiddenAliases": [],
      "id": "generate",
      "pluginAlias": "botarena",
      "pluginName": "botarena",
      "pluginType": "core",
      "strict": true,
      "enableJsonFlag": false,
      "isESM": true,
      "relativePath": [
        "dist",
        "cli",
        "commands",
        "generate.js"
      ]
    },
    "publish": {
      "aliases": [],
      "args": {},
      "description": "Publish bot profile to BotArena platform",
      "examples": [
        "<%= config.bin %> <%= command.id %> --config ./profile.json",
        "cat profile.json | <%= config.bin %> <%= command.id %>",
        "<%= config.bin %> <%= command.id %> --url https://staging.botarena.sh"
      ],
      "flags": {
        "config": {
          "char": "c",
          "description": "Path to bot configuration file",
          "name": "config",
          "required": false,
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "url": {
          "char": "u",
          "description": "BotArena API URL",
          "name": "url",
          "default": "https://botarena.sh",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "verbose": {
          "description": "Enable verbose logging output",
          "name": "verbose",
          "allowNo": false,
          "type": "boolean"
        }
      },
      "hasDynamicHelp": false,
      "hiddenAliases": [],
      "id": "publish",
      "pluginAlias": "botarena",
      "pluginName": "botarena",
      "pluginType": "core",
      "strict": true,
      "enableJsonFlag": false,
      "isESM": true,
      "relativePath": [
        "dist",
        "cli",
        "commands",
        "publish.js"
      ]
    }
  },
  "version": "0.0.8"
}