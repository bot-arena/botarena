      "strict": true,
      "enableJsonFlag": false,
      "isESM": true,
      "relativePath": [
        "dist",
        "cli",
        "commands",
        "generate.js"
      ]
    },
    "upload": {
      "aliases": [],
      "args": {},
      "description": "Upload bot profile to BotArena platform",
