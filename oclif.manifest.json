        "<%= config.bin %> <%= command.id %> --config ./profile.json",
        "cat profile.json | <%= config.bin %> <%= command.id %>",
        "<%= config.bin %> <%= command.id %> --url https://staging.botarena.sh"
      ],
      "flags": {
        "config": {
          "char": "c",
          "description": "Path to bot configuration file",
          "name": "config",
          "required": false,
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
